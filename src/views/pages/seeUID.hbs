<div style="margin-top: 25px;" class="container-md">

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">UID</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Acciones</th>

        </tr>
    </thead>
    <tbody>
        {{#each uids}}
        <tr>
            <th scope="row">{{this.uid}}</th>
            <td>{{name}}</td>
            <td>{{this.lastname}}</td>
            <td><a href="/delete/{{this.uid}}">Eliminar</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div>
